    <SolidColorBrush x:Key="MahApps.Brushes.TopMenuItem.PressedFill" Color="#566895" options:Freeze="True" />

    <!-- main menuitem hover norder -->
    <SolidColorBrush x:Key="MahApps.Brushes.TopMenuItem.PressedStroke" Color="#566895" options:Freeze="True" />
    <SolidColorBrush x:Key="MahApps.Brushes.TopMenuItem.SelectionStroke" Color="#566895" options:Freeze="True" />

    <!-- menuitem tekstkleur -->
    <Color x:Key="MahApps.Colors.Black">#ffffff</Color>

    <!-- tooltip borderkleur -->
    <Color x:Key="MahApps.Colors.Gray7">#07080a</Color>
    <SolidColorBrush x:Key="MahApps.Brushes.ThemeForeground" Color="#ffffff" options:Freeze="True" />

    <!-- oa klikken op min, max xlose buttons in header-->
