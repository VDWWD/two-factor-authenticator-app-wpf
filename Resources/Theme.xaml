    <Color x:Key="MahApps.Colors.Accent2">#ffff00</Color>
    <Color x:Key="MahApps.Colors.Accent3">#e9eefb</Color>
    <Color x:Key="MahApps.Colors.Accent4">#e9eefb</Color>



    <!--  Matadata  -->
    <system:String x:Key="Theme.Name">CustomAccentDarkRed</system:String>
    <system:String x:Key="Theme.DisplayName">CustomAccentDarkRed</system:String>
    <system:String x:Key="Theme.BaseColorScheme">Dark</system:String>
    <system:String x:Key="Theme.ColorScheme">#FFFF0000</system:String>
    <SolidColorBrush x:Key="Theme.ShowcaseBrush" Color="#Ffffffff" options:Freeze="True" />


