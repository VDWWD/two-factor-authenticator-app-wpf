    <!-- EWA: 8 digit hex: RR GG BB AA = alpha -->

    <!-- hoofdkleur -->
    <Color x:Key="MahApps.Colors.Accent">#566895</Color>

    <!-- menubalk kleur -->
    <Color x:Key="MahApps.Colors.White">#3e4254</Color>

    <!-- menuitem kleur -->
